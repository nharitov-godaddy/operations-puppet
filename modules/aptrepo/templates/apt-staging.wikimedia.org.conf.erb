server {
   listen 80 default_server;
   listen [::]:80 ipv6only=on default_server;
   server_name apt-staging.wikimedia.org;

   root /srv/aptrepo;

   location / {
      index index.html index.htm;
      autoindex on;
      charset utf-8;
   }

   location ~ ^/wikimedia-staging/(conf|db) {
      deny all;
   }
}
