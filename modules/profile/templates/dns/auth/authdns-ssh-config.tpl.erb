<%#- SPDX-License-Identifier: Apache-2.0 -%>
# This file is dynamically generated by confd. It only cares about the current
# DNS hosts and ignores what their pooled status is; that is defined by the
# list in /etc/wikimedia-authdns.conf.
{{- range gets "/authdns/*/*" }}
{{- $data := json .Value }}
{{- if ne $data.IP "127.0.0.1" }}
Host {{ base .Key }}
   HostName {{ $data.IP }}
{{- end }}
{{- end }}
