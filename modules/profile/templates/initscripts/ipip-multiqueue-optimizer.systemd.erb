<%#- SPDX-License-Identifier: Apache-2.0 -%>
[Unit]
Description=eBPF based IPIP Multiqueue Optimizer
After=network.target

[Install]
WantedBy=multi-user.target

[Service]
LimitMEMLOCK=infinity
ExecStart=/usr/bin/ipip-multiqueue-optimizer -p <%= @prometheus_addr %> -i <%= @optimizer_interfaces.join(",") %>
