[Service]
ExecReload=
ExecReload=/usr/sbin/php-fpm<%= @php_version %> -t
ExecReload=/bin/kill -USR2 $MAINPID
CPUAccounting=yes
<%- if @php_version == '7.4' -%>
# Override ExecStartPost for php7.4
ExecStartPost=
<%- end -%>
<%- if @increase_open_files -%>
LimitNOFILE=1024000
LimitNOFILESoft=1024000
<%- end -%>
