# global config for rawdog feeds
# shared among all language versions

splitstate true
maxarticles 30
xmlmaxarticles 100
maxage 0
expireage 1d
keepmin 3
currentonly true
daysections false

# This uses python datetime.strftime
dayformat %B %d, %Y
timesections false
timeformat %I:%M %p
datetimeformat %A, %e %B %Y %H:%M

userefresh true
showfeeds true

numthreads 7
timeout 30s

ignoretimeouts false
showtracebacks false
verbose false

blocklevelhtml true
tidyhtml true
sortbyfeeddate true
useids true

hideduplicates id
newfeedperiod 2h

changeconfig false
