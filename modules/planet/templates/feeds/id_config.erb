# Wikimedia related feeds in Indonesian language (id.planet.wikimedia.org)
include ../config
template /etc/rawdog/theme/wikimedia/id/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/id/rd_item.tmpl
outputfile /var/www/planet/id/index.html
outputxml /var/www/planet/id/rss20.xml
<% if @facts['os']['distro']['codename'] == 'bookworm' -%>
xmltitle Planet Wikimedia (<%= @title %>),
xmllink https//<%= @title %>.planet.wikimedia.org/,
xmllanguage <%= @title %>,
xmlurl https//<%= @title %>.planet.wikimedia.org/rss20.xml,
xmldescription aggregated feed of Wikimedia related blogs (<%= @title %> edition),
xmlownername Wikimedia,
xmlowneremail noc@wikimedia.org,
xmlmaxarticles 100,
<% end -%>
#

feed 1h https://ivanlanin.wordpress.com/category/wikipedia/feed/
