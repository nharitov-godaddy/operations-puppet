# Wikimedia related feeds in Greek language (el.planet.wikimedia.org)
include ../config
template /etc/rawdog/theme/wikimedia/el/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/el/rd_item.tmpl
outputfile /var/www/planet/el/index.html
outputxml /var/www/planet/el/rss20.xml
<% if @facts['os']['distro']['codename'] == 'bookworm' -%>
xmltitle Planet Wikimedia (<%= @title %>),
xmllink https//<%= @title %>.planet.wikimedia.org/,
xmllanguage <%= @title %>,
xmlurl https//<%= @title %>.planet.wikimedia.org/rss20.xml,
xmldescription aggregated feed of Wikimedia related blogs (<%= @title %> edition),
xmlownername Wikimedia,
xmlowneremail noc@wikimedia.org,
xmlmaxarticles 100,
<% end -%>
#

feed 1h https://magioladitis.blogspot.com/feeds/posts/default
feed 1h https://blog.wikimedia.gr/feed/
