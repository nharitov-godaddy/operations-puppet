# Wikimedia related feeds in Romanian language (ro.planet.wikimedia.org)
include ../config
template /etc/rawdog/theme/wikimedia/ro/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/ro/rd_item.tmpl
outputfile /var/www/planet/ro/index.html
outputxml /var/www/planet/ro/rss20.xml
<% if @facts['os']['distro']['codename'] == 'bookworm' -%>
xmltitle Planet Wikimedia (<%= @title %>),
xmllink https//<%= @title %>.planet.wikimedia.org/,
xmllanguage <%= @title %>,
xmlurl https//<%= @title %>.planet.wikimedia.org/rss20.xml,
xmldescription aggregated feed of Wikimedia related blogs (<%= @title %> edition),
xmlownername Wikimedia,
xmlowneremail noc@wikimedia.org,
xmlmaxarticles 100,
<% end -%>
#

feed 1h https://www.strainu.ro/category/projects/wikipedia-projects/feed/
feed 1h https://promitt.wordpress.com/category/wikipedia/feed/atom/
feed 1h https://rowiki.wordpress.com/feed/atom/
feed 1h https://www.smash.ro/teme/wikipedia/feed/
feed 1h https://wikilovesmonuments.ro/feed/
