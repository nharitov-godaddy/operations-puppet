# Wikimedia related feeds in Czech language (cs.planet.wikimedia.org)
include ../config
template /etc/rawdog/theme/wikimedia/cs/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/cs/rd_item.tmpl
outputfile /var/www/planet/cs/index.html
outputxml /var/www/planet/cs/rss20.xml
<% if @facts['os']['distro']['codename'] == 'bookworm' -%>
xmltitle Planet Wikimedia (<%= @title %>),
xmllink https//<%= @title %>.planet.wikimedia.org/,
xmllanguage <%= @title %>,
xmlurl https//<%= @title %>.planet.wikimedia.org/rss20.xml,
xmldescription aggregated feed of Wikimedia related blogs (<%= @title %> edition),
xmlownername Wikimedia,
xmlowneremail noc@wikimedia.org,
xmlmaxarticles 100,
<% end -%>
#

feed 1h https://blog.wikimedia.cz/feed/atom/
feed 1h https://wikipediste.blogspot.com/feeds/posts/default
feed 1h https://okinokynko.blogspot.com/feeds/posts/default/-/wiki
feed 1h https://okinokynko.blogspot.com/feeds/posts/default/-/Wikipedie
