# Wikimedia related feeds in Spanish language (es.planet.wikimedia.org)
include ../config
template /etc/rawdog/theme/wikimedia/es/rd_page.tmpl
itemtemplate /etc/rawdog/theme/wikimedia/es/rd_item.tmpl
outputfile /var/www/planet/es/index.html
outputxml /var/www/planet/es/rss20.xml
<% if @facts['os']['distro']['codename'] == 'bookworm' -%>
xmltitle Planet Wikimedia (<%= @title %>),
xmllink https//<%= @title %>.planet.wikimedia.org/,
xmllanguage <%= @title %>,
xmlurl https//<%= @title %>.planet.wikimedia.org/rss20.xml,
xmldescription aggregated feed of Wikimedia related blogs (<%= @title %> edition),
xmlownername Wikimedia,
xmlowneremail noc@wikimedia.org,
xmlmaxarticles 100,
<% end -%>
#

